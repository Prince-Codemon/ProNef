    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/proneflogo.png",
        
        "backgroundColor": "#4D626C"
      }
    },
    "web": {
      "favicon": "./assets/proneflogo.png"
    }
  }
}
