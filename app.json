{
  "expo": {
    "name": "ProNef",
    "slug": "ProNef",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/proneflogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/pronefsplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
